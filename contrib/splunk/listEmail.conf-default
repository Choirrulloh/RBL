;Threshold value on trigger condition; the same which engage the alert
threshold = 2

;Blacklist name
typedesc  = 'Spam Sender'

;How long to list parameters in MySQL language ;)
unit = 'DAY'
quantity = 2

;Syslog
tag      = 'Email'

;Splunk password of the username who own the webhook alert.
;You must know it.
splunkpassword = 'changeme'
;Splunk daemon port
splunkport = 8089
